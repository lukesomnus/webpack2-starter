  "license": "MIT",
  "engines": {
    "node": ">=6.8"
  },
  "scripts": {
    "clean:dist": "rimraf -- dist",
    "start": " npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server  --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start --reporters dots",
    "build:dev": "webpack-dev-server --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.26.0",
    "jade-loader": "^0.8.0",
    "koa": "^1.2.4",
    "koa-router": "^7.1.0",
    "node-sass": "^4.1.1",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.3",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "normalize.css": "^5.0.0",
    "pug": "^2.0.0-beta6"
  }
}